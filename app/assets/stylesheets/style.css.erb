html {
    height: 100%;
	margin: 0px;
	padding: 0px;
}

body {
    height: 100%;
    margin: 0px;
	padding: 0px;
    background-repeat: no-repeat;
	
	font-family: "Helvetica Neue", Helvetica, verdana, arial, helvetica, sans-serif;
	font-size: 13px;
	background-color: #ededed;
	color: #3F4554;
	text-align: left;
	
	background-color:#F5F5F5;
	background-image: -moz-radial-gradient(50% 50%, ellipse closest-side, #ffffff 68%,#e4e4e4 150%);
	background-image: -webkit-radial-gradient(50% 50%, ellipse closest-side, #ffffff 68%,#e4e4e4 150%);
	background-image: -o-radial-gradient(50% 50%, ellipse closest-side, #ffffff 68%,#e4e4e4 150%);
	background-image: -ms-radial-gradient(50% 50%, ellipse closest-side, #ffffff 68%,#e4e4e4 150%);
	background-image: radial-gradient(50% 50%, ellipse closest-side, #ffffff 68%,#e4e4e4 150%);
}

body, p, ol, ul, td {
  line-height: 1.8em;
}

pre {
  background-color: #eee;
  padding: 10px;
  font-size: 11px;
}

a { 
	color: #056ba7;
	text-decoration: none;
}

a:visited { 
	color: #056ba7;
	text-decoration: none;
}

a:hover {
	text-decoration: underline;
}

div.field, div.actions {
  margin-bottom: 10px;
}

.field select {
	width: 220px;
}

#notice {
  color: green;
}

.notice {
	border: 1px solid #CCC;
	padding: 10px;
	background-color: #fffbc1;
}

.field_with_errors {
  padding: 2px;
  background-color: red;
  display: table;
}

#error_explanation {
  width: 450px;
  border: 2px solid red;
  padding: 7px;
  padding-bottom: 0;
  margin-bottom: 20px;
  background-color: #f0f0f0;
}

#error_explanation h2 {
  text-align: left;
  font-weight: bold;
  padding: 5px 5px 5px 15px;
  font-size: 12px;
  margin: -7px;
  margin-bottom: 0px;
  background-color: #c00;
  color: #fff;
}

#error_explanation ul li {
  font-size: 12px;
  list-style: square;
}

#header {
	border-bottom: 1px solid #dadada;
	padding-top: 5px;
	padding-bottom: 5px;
}

#header .logo h1 a {
	
	background-image: url(<%= asset_path 'logo.png' %>);
	display: block;
	width: 256px;
	height: 46px;
	z-index: -1;
	text-indent: -3000px;
	white-space: nowrap;
	text-decoration: none;
	outline: none;
	overflow: hidden;
}

#header .logo h1 a:hover {
	text-decoration: none;
	background-color: transparent;
	opacity: 0.6;
}

#wrapper {
	width: 100%;
	margin: 15px auto 15px auto;
	padding: 0px;
	text-align: left;
	border: solid 1px #dadada;
	background-image: url(<%= asset_path 'divider.gif' %>);
	background-repeat: repeat-y;
	background-position: center;
}

#wrapper_profile {
	display: inline-block;
	margin: 15px 0px 15px 0px;
	padding: 15px;
	text-align: left;
	border: solid 1px #dadada;
	background-color: #F5F5F5;
	background-image: url(<%= asset_path 'grain.png' %>);
	background-repeat: repeat;
}

#wrapper_global {
	margin: 15px 0px 15px 0px;
	padding: 15px;
	text-align: left;
	border: solid 1px #dadada;
	background-color: #F5F5F5;
	background-image: url(<%= asset_path 'grain.png' %>);
	background-repeat: repeat;
}

#wrapper_profile h1, #wrapper_global h1 {
	margin: 0px 0px 15px 0px;
	text-shadow: 0px 1px 0px #FFF;
}

#wrapper_global h2 {
	padding: 0px;
	margin: 0px;
}

#header_inner, #header_form {
	width: 100%;
	/*background-color: #f1f1f1;*/
	background-color: #F5F5F5;
	border-bottom: solid 1px #dadada;
	padding-top: 1px;
	padding-bottom: 5px;
	background-image: url(<%= asset_path 'grain.png' %>);
	background-repeat: repeat;
}

#header_inner {
	height: 450px;
}

#header_form {
	height: 250px;
}

#header_inner, #header_form input {
	font-size: 14px;
}

#header_inner h2 { 
	font-family: "Helvetica Neue Light", "HelveticaNeue-Light", Helvetica, Arial, sans-serif;
	font-size: 30px;
	line-height: 36px;
	padding: 15px 0px 4px 0px;
	margin: 0px;
	font-weight: normal;
}

#header_form h2 {
	font-family: "Helvetica Neue Light", "HelveticaNeue-Light", Helvetica, Arial, sans-serif;
	font-size: 30px;
	line-height: 36px;
	padding: 15px 0px 12px 0px;
	margin: 0px;
	font-weight: normal;	
}

#header_inner h2 .hilite {
	color: #f58120;
}

#header_form h2 .hilite {
	color: #f58120;
}

.buttons a:hover {
	opacity: 0.65;
}

.hidden {
	display: none;
}

#container {
	width: 950px;
	height: auto;
	margin: 0 auto;
	padding: 2px 0px;
}

#container_inner {
	width: 100%;
	height: auto;
	margin: 0px;
	padding: 2px 0px;
	background-image: url(<%= asset_path 'grain.png' %>);
	background-repeat: repeat;
}

#topbar {
	font-family: "Lucida Grande", Verdana, Arial, sans-serif;
	font-size: 11px;
	padding: 5px 0px 0px 0px;
