#left {
	width: 450px;
	height: auto;
	float: left;
	display: inline-block;
	padding: 10px;
	margin: 0px;
}

#left_editing {
	width: 400px;
	display: inline-block;
	padding: 10px;
	margin: 0px;
}

#right {
	width: 450px;
	height: auto;
	float: right;
	display: inline-block;
	padding: 10px;
	margin: 0px;
}

#content {
	margin-left: 230px;
	width: 688px;
}

#sidebar {
	width: 220px;
	margin: 0px;
	padding: 0px;
	float: left;
	overflow: scroll;
}

#sidebar h2 {
	padding: 0px;
	margin-top: 5px;
}

#sidebar h3 {
	padding: 5px 0px 0px 0px;
	margin-top: 0px;
	margin-bottom: 1px;
	margin-right: 20px;
	border-top: 1px solid #CCC;
}

#profilepicture {
	
}

.clearer {
font-size: 0px;
height: 0px;	
width: 100%;
display: block;
clear: both;
}

#nav {
margin: 0px;
padding: 0px;
list-style: none;
list-style-image: none;
}

#nav li {
margin: 0px;
padding: 0px;
