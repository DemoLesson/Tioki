#footer {
	font-family: "Lucida Grande", Verdana, Arial, sans-serif;
	border-top: 1px solid #dadada;
	font-size: 11px;
	padding-bottom: 8px;
}

#footer p {
	font-weight: bold;
}

#footer .copyright {
	font-size: 10px;
}

#footer .copyright a {
	color: #666;
	font-weight: bold;
}

