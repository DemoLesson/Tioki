<% if self.current_user.nil? %>
    <%= render :partial => 'splash' %>
<% elsif ['teacher', nil, String.new].include?(params[:dashboard] || self.current_user.dashboard)  %>
    <%= render :partial => 'teacher_dashboard' %>
<% elsif ['school'].include?(params[:dashboard] || self.current_user.dashboard) %>
    <%= render :partial => 'school_dashboard' %>
<% end %>
