<% if currentUser.new_record? %>
	<% if @ab == "0" %>
		<%= render :partial => 'splash' %>
	<% else %>
		<%= render :partial => 'splash_b' %>
	<% end %>
<% elsif ['recruiter'].include?(currentUser.dashboard) %>
	<%= render :partial => 'recruiter_dashboard' %>
<% else %>
	<%= render :partial => 'educator_dashboard' %>
<% end %>
